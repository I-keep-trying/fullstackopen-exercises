{
  "name": "react-boilerplate",
  "version": "0.1.0",
  "private": true,
  "repository": "https://github.com/I-keep-trying/create-react-app-boilerplate",
  "dependencies": {
    "axios": "^0.19.2",
    "prop-types": "^15.7.2",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-scripts": "^3.4.3"
  },
  "scripts": {
    "start": "set PORT=3001 && react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "git": "git add . && git commit -m updates && git push origin master",
    "eslint": "eslint .",
    "cypress:open": "cypress open"
  },
  "proxy": "http://localhost:3007",
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.11.3",
    "@testing-library/react": "^10.4.8",
    "cypress": "^5.0.0",
    "eslint-plugin-jest": "^23.20.0"
  }
}
